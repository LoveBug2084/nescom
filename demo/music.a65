SetNoteLengthWithTickSpeed_SetupTone:
  ; Get the note length
  ;   ;76543210 c
  rol ;6543210c 7
  rol ;543210c7 6
  rol ;43210c76 5
  rol ;3210c765 4
  and #15 ; got the 3 high bits of X. 4*ROL is faster than 5*LSR.
  ;         Because X was 10..1F or 40..FF,
  ;         A can be 0..7 except 1
  tay
  lda NoteLengths,y  ; One of: 0,2,4,8,16,32,64
  ; or if carry was set:       0,3,6,12,24,48,96

  ; Sets CHNVAR_NoteLengthWord as param * CHNVAR_TickSpeed
  ; Uses TempF4..$F5 as temps, preserves X
  sta TempF4
  stx TempF4+1

  ldy #CHNVAR_TickSpeed
  lda (CurChanDataPtr),y
  tay

  ; Set X:A = Y*TempF4

  lda #$00
  tax
  clc
- adc TempF4
  bcc +
  inx
  clc
+ dey
  bne -

  ; Write X:A to NoteLengthWord
  ldy #CHNVAR_NoteLengthWord
  sta (CurChanDataPtr),y
  iny
  txa
  sta (CurChanDataPtr),y
  
  ldx TempF4+1
  ;fallthru
