  lda AudioStatusBuffer+CHNVAR_NoteLengthWord+0,x
  sec : sbc #4
  sta AudioStatusBuffer+CHNVAR_NoteLengthWord+0,x
  bcs +
  dec AudioStatusBuffer+CHNVAR_NoteLengthWord+1,x
