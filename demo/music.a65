  sta (CurChanDataPtr),y
  bne ReadDataMUS

CMDMUS_SetNoteDelay:
  ; It was 20-27 (or 28-2F, which works identically to 20-27).
  ; Handle set-notedelay command
  txa
  pha
   jsr ReadDataMUS
