;
;
; This file can be assembled into IPS or O65
; using Nescom - http://bisqwit.iki.fi/source/nescom.html
; and then reinserted into the ROM with a tool of choice.
;
; Note: Mega Man 2 has almost the same code!
;

.zero
	Priorities:            .byt 0
; $E0 = #ssssmmmm
; mmmm = Music priority (always #$F?...)
; ssss = sfx   priority

	SFX_ChannelUsage:      .byt 0
; $E1 = #????3210
; channels used for SFX
;

	SongDataPointer:       .word 0 ;At $E2,E3
	SuspendPlayback:       .byt 0  ;$E4
	ChannelsRemaining:     .byt 0  ;$E5
	PriorityTemp = ChannelsRemaining ;alias
	;RegisterOffsetUnknown: .byt 0 ; $E6 ; no longer needed
	SpeedUpFactor:         .byt 0 ; $E7 ; If 2, each note is played at half length!
	FadingSpeed:           .byt 0 ; $E8 ; Set by Command $FD
	VolumeFadeStrength:    .byt 0 ; $E9 ; Controlled by FadingSpeed
	SoundFrameCounter:     .byt 0 ; $EA
	;CurrentChannelRegisterOffset: .byt 0 ; $EB ; no longer needed
	CurChanDataPtr:        .word 0 ; $EC ; EC,ED   it's an index to AudioStatusBuffer
	CurrentChannelNumber:  .byt 0 ; $EE
	UnknownEF:             .byt 0 ; $EF
	SFX_Addr:              .word 0 ; $F0 ; F0,F1
	SFX_FrameWaitCount:    .byt 0 ; $F2 ; Number of frames before next SFX tick
	SFX_LoopCounter:       .byt 0 ; $F3
	TempF4: .word 0 ;$F4

#define MUSCMD_SetSpeed              0
#define MUSCMD_SetVibratoCtrl        1 ;probably wrong
#define MUSCMD_SetDutyCycle          2
#define MUSCMD_SetVolumeAndEnv       3
#define MUSCMD_Loop                  4
#define MUSCMD_SetNoteBaseOffset     5
#define MUSCMD_PlayNoteExtraLength   6   ;implied
#define MUSCMD_SetVolumeCurve        7
#define MUSCMD_SetVibratoIndex       8
#define MUSCMD_EndOfData             9
#define MUSCMD_SetNoteDelay          $20
#define MUSCMD_TripletSpeed          $30 ;implied

#define SFXCMD_SetFrameWaitCount     0
#define SFXCMD_SetVibratoCtrl        1
#define SFXCMD_SetDutyCycle          2
#define SFXCMD_SetVolumeAndEnv       3
#define SFXCMD_Loop                  4
#define SFXCMD_SetVibratoBuffer      5
#define SFXCMD_EndOfData             6
#define SFXCMD_SetNotePeriod         $80
#define SFXCMD_Exec_80               $80

; Each channel data is 31 bytes ($1F)
#define CHNVAR_MusicDataAddr          $00 ; Pointer to the current executing position of the track
#define CHNVAR_NoteLengthWord         $02 ; This value is only read at 96DF
#define CHNVAR_TickSpeed              $04
#define CHNVAR_LoopCounter            $05
#define CHNVAR_VibratoDefs            $06 ;high 3 bits: delay in pitch? low 5 bits: vibrato depth
#define CHNVAR_NoteBaseOffset         $07 ;word
#define CHNVAR_VibratoCtrl            $09 ;probably wrong
#define CHNVAR_NotePeriodWord         $0A ;looks right
#define CHNVAR_DutyCycle              $0C ;high two bits=duty, low 6=volume and env
#define CHNVAR_VolumeCurveLength      $0D ;00..7F=ascending, 80..FF=descending 
#define CHNVAR_VolumeCurvePosition    $0E
#define CHNVAR_VolumeCurveStrength    $0F ;high 4 bits used only
#define CHNVAR_VibratoCtrlSFX	      $10 ;probably wrong
#define CHNVAR_NotePeriodWordSFX      $11 ;looks right
#define CHNVAR_DutyCycleSFX           $13
#define CHNVAR_VibratoBuffer          $14 ;14,15,16,17
#define CHNVAR_VibratoVar1            $18
#define CHNVAR_VibratoVar2            $19
#define CHNVAR_PeriodWord             $1A ; Added together with NotePeriodWord when written
#define CHNVAR_PeriodHiOld            $1C ; Cache of PeriodWord+1
#define CHNVAR_TremoloVar1            $1D
#define CHNVAR_TremoloVar2            $1E
#define AudioChannelStatusSize        $1F ;31 bytes

.bss
VibratoBufferPointer: .word 0                       ; $57C ;pointer to the "fifth" channel data
AudioStatusBuffer:    .nop 4*AudioChannelStatusSize ; $500..$57C


; Vibrato bytes;
;   First byte seems to be the vibrato wavelength (aka. speed)
;   Second byte:
;     High nibble: Vibrato depth
;     Low nibble: Vibrato wavelength (again?)
;   Third byte:
;     Tremolo depth | $80
;   Fourth byte:
;     Tremolo speed? 0=disabled

.zero
	MusicPaused:		.byt 0 ;At $41 If this address has low bit set, the engine pauses music (read only)

.text

;SoundCodePlay ; at $8000
;  jmp UpdateSound

SoundCodeInit ; at $8003
  ; 00..FB = select song
  ; FC     = Speed up music by factor "Y"
  ; FD     = Fade in (-Y) or fade out (+Y)
  ; FE     = Reset soundsystem
  ; FF     = Stop music
  cmp #$FC
  bcc NormalSoundCodeCommand
  beq SoundCodeCommandFC
  cmp #$FE
  bcs SoundCodeCommandFE_FF
SoundCodeCommandFD ; FADE IN (-Y) OR FADE OUT (+Y)
  sty FadingSpeed
  lda #$01 : sta VolumeFadeStrength
  lda SoundFrameCounter
  and #$01
  sta SoundFrameCounter
  rts

SoundCodeCommandFE_FF:
  beq SoundCodeCommandFE
SoundCodeCommandFF ; STOP MUSIC
  jsr StopMusic
  ;fallthrough
ResetChanDataPtr:
  lda #$01                : sta SuspendPlayback
  lda #<AudioStatusBuffer : sta CurChanDataPtr
  rts
LoadChanDataPtr_X:
  lda CurChanDataPtr
  sec
  sbc #<AudioStatusBuffer
  tax
  rts

SoundCodeCommandFC ; SPEED UP MUSIC BY FACTOR "Y"
  iny
  sty SpeedUpFactor
  rts

SoundCodeCommandFE ; RESET SOUNDSYSTEM
  jsr ResetChanDataPtr
  jmp ResetSoundSystem


NormalSoundCodeCommand:
  asl
  tax
  lda MusicAndSfxTable+0,x : sta SongDataPointer+0
  lda MusicAndSfxTable+1,x : sta SongDataPointer+1
  ldy #$00
  lda (SongDataPointer),y
  tax
  and #$0F
  beq StartSFX


StartMusicTrack:
; If this music score has lower priority than the currently playing one,
; then just return...
  lda Priorities
  and #$0F
  sta PriorityTemp
  cpx PriorityTemp
  bcs +
  rts
+
; Set the priority to this new music
  stx PriorityTemp
  lda Priorities
  and #$F0
  ora PriorityTemp
  sta Priorities

; Suspend playback so the sound code won't interrupt us
  jsr ResetChanDataPtr
  lda #$00                : sta SpeedUpFactor
  sta FadingSpeed

  lda #$04    ; Number of channels to loop thru
  sta ChannelsRemaining

--
  ; Copy track data pointer
  jsr LoadChanDataPtr_X
  ldy #0
  
  inc SongDataPointer+0 : bne + : inc SongDataPointer+1 : +
  lda (SongDataPointer),y : sta AudioStatusBuffer+CHNVAR_MusicDataAddr+0,x

  inc SongDataPointer+0 : bne + : inc SongDataPointer+1 : +
  lda (SongDataPointer),y : sta AudioStatusBuffer+CHNVAR_MusicDataAddr+1,x
  
; Clear rest of the channel's vars (02..0F). 10..1E is left intact.
  lda #0
  ldy #14
- sta AudioStatusBuffer+2,x : inx : dey : bne -

  lda SFX_ChannelUsage : lsr : bcs +
  jsr ClearSFXRAM
+ jsr SFX_NextChannel
  
  dec ChannelsRemaining : bne --

  jsr SFX_FirstChannel

  ; All channels processed.
  ; Copy the vibrato data pointer.
  ldy #0
  inc SongDataPointer+0 : bne + : inc SongDataPointer+1 : +
  lda (SongDataPointer),y : sta VibratoBufferPointer+0

  inc SongDataPointer+0 : bne + : inc SongDataPointer+1 : +
  lda (SongDataPointer),y : sta VibratoBufferPointer+1

; Let music callback play again
  lda #0 : sta SuspendPlayback
  rts


StartSFX: ; at 80BB
; If SFX priority has a lower priority then the current one, then exit
  lda Priorities
  and #$F0
  sta PriorityTemp
  cpx PriorityTemp
  bcs +
  rts
+
; Set the sfx priority to this new sfx
  stx PriorityTemp
  lda Priorities
  and #$0F
  ora PriorityTemp
  sta Priorities
; Suspend playback so the sound code won't interrupt us
  jsr ResetChanDataPtr
  
  ldx #0
  
  ; Set SFX_Addr as SongDataPointer+2
  lda #$02
  clc
  adc SongDataPointer+0
  sta SFX_Addr+0
  txa
  adc SongDataPointer+1
  sta SFX_Addr+1

  stx SFX_FrameWaitCount
  stx SFX_LoopCounter

; ChannelUsage
  ldy #$01
  lda (SongDataPointer),y
  and #$0F
  tax
  ora SFX_ChannelUsage
  pha
  stx SFX_ChannelUsage

; Loop thru all channels
  lda #$04 : sta ChannelsRemaining

--
  pla
  lsr
  pha
  bcc +

  ; this SFX uses this channel so clear SFX RAM
  jsr ClearSFXRAM

  lda SFX_ChannelUsage : lsr : bcs +
  jsr UpdateChannelUsedBySFX
+ jsr SFX_NextChannel

  dec ChannelsRemaining
  bne --

  jsr SFX_FirstChannel

  lda SFX_ChannelUsage
  sta UnknownEF
  pla


; Let music callback play again
  lda #0 : sta SuspendPlayback
  rts



ResetSoundSystem ;at 813A (diff so far: 5 bytes)
; Set SFX priority to 0
  lda Priorities
  and #$0F
  sta Priorities

  lda #4 : sta ChannelsRemaining

--
  lda SFX_ChannelUsage : lsr : bcc +
  jsr ClearSFXRAM
  jsr UpdateChannelUsedBySFX
+ jsr SFX_NextChannel

  dec ChannelsRemaining
  bne --

  lda #0
  sta SFX_ChannelUsage
  sta UnknownEF
  lda #0
  sta SuspendPlayback
  rts


;Loads a SFX-related pointer(?)
UpdateChannelUsedBySFX ; at 816C
  jsr LoadChanDataPtr_X
  pha
   lda AudioStatusBuffer+CHNVAR_NotePeriodWord+0,x   ;If NotePeriodWord is nonzero?
   ora AudioStatusBuffer+CHNVAR_NotePeriodWord+1,x
   bne pla_ReloadVibrato

   ldy ChannelsRemaining       ;NotePeriodWord was zero.
   jsr HW_MuteChannel_WithY

  pla
  lda AudioStatusBuffer+CHNVAR_MusicDataAddr+0,x   ;Reads MusicDataAddr
  ora AudioStatusBuffer+CHNVAR_MusicDataAddr+1,x
  bne ReloadVibrato
  rts

; This routine copies the data from the currently selected
; vibrato buffer of the music track into the 14..17 range
; in the channel.
;
; It is the same region as updated by SFXCMD_SetVibratoBuffer.
;
pla_ReloadVibrato:
   pla
ReloadVibrato ; at 81C4 - 8 bytes ahead here
   ldy #CHNVAR_VibratoDefs
   lda (CurChanDataPtr),y
   and #$1F
   tax
  ; passthru
CopySelectedVibratoData ;at 88E1
  txa
  asl : asl ; multiply by 4
  adc VibratoBufferPointer+0
  sta TempF4
  lda #0
  adc VibratoBufferPointer+1
  sta TempF4+1

  ldx #0
  ldy #CHNVAR_VibratoBuffer+0
- lda (TempF4,x)
  sta (CurChanDataPtr),y
  iny
  cpy #CHNVAR_VibratoBuffer+4
  beq +
  inc TempF4+0 : bne -
  inc TempF4+1 : bne -
+ rts


StopMusic ;at 818C ;7 bytes ahead here
; Set music priority to 0
  lda Priorities
  and #$F0
  sta Priorities

; Loop thru all channels
  lda #0
  sta SpeedUpFactor
  sta FadingSpeed
  tax
  jsr +
  inx
  sta SuspendPlayback
+ sta AudioStatusBuffer+(AudioChannelStatusSize*0)+CHNVAR_MusicDataAddr,x
  sta AudioStatusBuffer+(AudioChannelStatusSize*1)+CHNVAR_MusicDataAddr,x
  sta AudioStatusBuffer+(AudioChannelStatusSize*2)+CHNVAR_MusicDataAddr,x
  sta AudioStatusBuffer+(AudioChannelStatusSize*3)+CHNVAR_MusicDataAddr,x
  rts


ClearSFXRAM ;Zeroes the region $10..$1E of the current channel. 
  ldy #$0F
  jsr LoadChanDataPtr_X
  lda #$00
- sta AudioStatusBuffer+$10,x
  inx
  dey
  bne -
  rts


; Rotate SFX_ChannelUsage right without involving carry
SFX_NextChannel ; at 8207
  lsr SFX_ChannelUsage
  bcc NextChannelPtr
  lda SFX_ChannelUsage
  ora #$80
  sta SFX_ChannelUsage
NextChannelPtr
  ; CurChanDataPtr += 31
  lda #AudioChannelStatusSize
  clc
  adc CurChanDataPtr
  sta CurChanDataPtr
  rts


SFX_FirstChannel ; at 8219
  lsr SFX_ChannelUsage
  lsr SFX_ChannelUsage
  lsr SFX_ChannelUsage
  lsr SFX_ChannelUsage
  rts


; Mutes channel Y by setting period to #0 (or disabling it if noise)
HW_MuteChannel ;at 9508
  ldy CurrentChannelNumber
HW_MuteChannel_WithY:
  cpy #1
  beq +       ; if chn# = (last channel) then disable noise channel
  ; X = (4-Y)*4 + 2
  ; X = 16 - Y*4 + 2
  ; X = 18 - Y*4
  ldx ChannelOffsets-1,y
  ; Write $0000 as the period value of the channel
  lda #0
  sta $4002,x
  sta $4003,x
  rts
+ lda #$07    ; Disable noise channel
  sta $4015
  rts


UpdateSound ;at 8235 - 10 bytes ahead
SoundCodePlay = UpdateSound ; at $8000
  inc SoundFrameCounter                               ; $EA
  lda SuspendPlayback
  beq +
  rts
+
  lda #<AudioStatusBuffer : sta CurChanDataPtr+0
  lda #>AudioStatusBuffer : sta CurChanDataPtr+1
; Loop thru all channels
  lda #$04
  sta CurrentChannelNumber

UpdateSound_ChannelLoop:
  jsr LoadChanDataPtr_X
  inc AudioStatusBuffer+CHNVAR_VibratoVar1,x ;adds 1 to VibratoVar1
  inc AudioStatusBuffer+CHNVAR_TremoloVar1,x ;adds 1 to TremoloVar1
; Call SFX_TickChannel if bit set in UnknownEF for this channel...
  lda UnknownEF
  lsr
  bcc +
  jsr SFX_TickChannel
+
; Jump if MusicPaused has low bit set
  lda MusicPaused
  lsr
  bcc +
  jmp ++
+
; Skip processing if music address = 0
  jsr LoadChanDataPtr_X
  lda AudioStatusBuffer+CHNVAR_MusicDataAddr+0,x
  ora AudioStatusBuffer+CHNVAR_MusicDataAddr+1,x
  beq ++
  inc AudioStatusBuffer+CHNVAR_VolumeCurvePosition,x ;adds 1 to VolumeCurvePosition
  jsr MUS_TickChannel
  jmp +++
; Music data for this channel is missing, so mute it
++
  lda UnknownEF
  lsr
  bcs +++
   jsr HW_MuteChannel
+++
; Rotate UnknownEF right without involving carry
  lsr UnknownEF
  bcc +
  lda UnknownEF
  ora #$80
  sta UnknownEF
+
  dec CurrentChannelNumber
  beq UpdateSound_AllChannelsDone
  
  jsr NextChannelPtr
  jmp UpdateSound_ChannelLoop

UpdateSound_AllChannelsDone:
; Do something that semi-resets frame counter sometimes (not very often)
  lda FadingSpeed
  and #$7f
  beq ++
  cmp SoundFrameCounter
  bne ++
  lda SoundFrameCounter
  and #$01
  sta SoundFrameCounter
  inc VolumeFadeStrength
  lda #$10
  cmp VolumeFadeStrength
  bne ++
  lda FadingSpeed
  bmi +
  lda #0
  sta FadingSpeed
+
  lda #$0F
  sta VolumeFadeStrength
++
; This seems to be the length of an SFX or something of the like...
  lda SFX_FrameWaitCount
  beq +
  dec SFX_FrameWaitCount
+
  lsr UnknownEF
  lsr UnknownEF
  lsr UnknownEF
  lsr UnknownEF
  rts


IterateNote ;at 82EC
; This function is arrived when
; both NoteLength > 0 and NotePeriodWord > 0.
;
  ldy #CHNVAR_DutyCycle
  lda (CurChanDataPtr),y
  ldy #$02
  cpy CurrentChannelNumber
  beq +      ;Skip 'and #$0F' for triangle channel
  and #$0F
+
  sta TempF4
  lda FadingSpeed
  and #$7F
  beq ++
  lda VolumeFadeStrength

  ldy #$02
  cpy CurrentChannelNumber ;triangle channel can't be faded
  bne +
  ldx #$0c
-
  clc
  adc VolumeFadeStrength
  dex : bne -
+
  tay
  lda FadingSpeed
  bmi +
  ldx #$ff
-
  inx
  cpx TempF4 : beq ++
  dey     : bne -
  stx TempF4
  jmp ++
+ -
  dec TempF4 : beq ++
  dey     : bne -
++
  lda #$02
  cmp CurrentChannelNumber
  beq DoneVolumeCurve
  ldy #CHNVAR_VolumeCurveLength
  lda (CurChanDataPtr),y
  tax
  and #$7F
  beq DoneVolumeCurve

; This part omitted if channel is triangle or VolumeCurveLength had no low 7 bits
  iny
  cmp (CurChanDataPtr),y    ;VolumeCurvePosition
  beq +
  iny
  lda (CurChanDataPtr),y    ;VolumeCurveStrength
  and #$0F
  jmp ++
+
  lda #$00
  sta (CurChanDataPtr),y    ;VolumeCurvePosition
  iny
  lda (CurChanDataPtr),y    ;VolumeCurveStrength
  lsr
  lsr
  lsr
  lsr
  sta TempF4+1              ; $F5 <- (VolumeCurveStrength >> 4)
  txa                       ; A = VolumeCurveLength including high bits
  bpl +
  lda #$00
  sec
  sbc TempF4+1
  sta TempF4+1              ; $F5 = 0 - $F5
+
  lda (CurChanDataPtr),y    ; VolumeCurveStrength
  and #$0F                  ; low 4 bits thereof
  clc
  adc TempF4+1
  bpl ++
  lda #$00
  jmp +++
++
  cmp TempF4: bcc +++
  lda TempF4
+++
  sta TempF4
  lda (CurChanDataPtr),y    ; VolumeCurveStrength
  and #$F0
  ora TempF4
  sta (CurChanDataPtr),y    ; VolumeCurveStrength

; Arrived here if channel was triangle or VolumeCurveLength had no low 7 bits
DoneVolumeCurve
  lda UnknownEF
  lsr
  bcs +
   lda #CHNVAR_DutyCycle   : sta TempF4+1
   jmp ProcessTremolo
+
  lda #CHNVAR_VibratoCtrl : sta TempF4+1
  jmp ProcessVibrato

ProcessTremolo ;volume vibrato
   ; $F5 points to DutyCycle or DutyCycleSFX|$80

  ldy #CHNVAR_VibratoBuffer+2
  lda (CurChanDataPtr),y
  and #$7F
  beq +++++
    ldy #CHNVAR_TremoloVar1
    cmp (CurChanDataPtr),y
    beq +
    jmp ++++
    +
      lda #$00
      sta (CurChanDataPtr),y
      ldy #CHNVAR_VibratoBuffer+3
      lda (CurChanDataPtr),y
      ldy #CHNVAR_TremoloVar2
      clc
      adc (CurChanDataPtr),y
      beq +
      bpl ++
      +
        lda #$01
        sta (CurChanDataPtr),y
        jmp +++
      ++
        sta (CurChanDataPtr),y
        cmp #$10
        bcc ++++
          lda #$0F
          sta (CurChanDataPtr),y
    +++
      lda #$00
      ldy #CHNVAR_VibratoBuffer+3
      sec
      sbc (CurChanDataPtr),y
      sta (CurChanDataPtr),y
  ++++
    ldy #CHNVAR_TremoloVar2
    lda (CurChanDataPtr),y
    cmp TempF4
    bcs +++++
    sta TempF4
+++++
  ldy #$02 : cpy CurrentChannelNumber : beq +
   lda TempF4+1
   and #$7f
   tay
   lda (CurChanDataPtr),y
   and #$f0
   ora TempF4
   sta TempF4
+
  ldy CurrentChannelNumber
  ldx ChannelOffsets-1,y
  lda TempF4
  sta $4000,x ;Write duty,decaysettings,length_disable

  lda TempF4+1 ; bit 7 set means SFX channel
  bpl +
  lda #CHNVAR_VibratoCtrlSFX | $80 : sta TempF4+1
  jmp ProcessVibrato
+
  lda #CHNVAR_VibratoCtrl : sta TempF4+1

ProcessVibrato
  ; $F5 points to VibratoCtrl or VibratoCtrlSFX|$80

  lda TempF4+1
  and #$7F
  tay
  ldx #$00
  lda (CurChanDataPtr),y
  beq ++
    bpl +
    dex
  + iny ;Either NotePeriodWord or NotePeriodWordSFX
    ; Add the word A:X into that word
  
    clc
    adc (CurChanDataPtr),y
    sta (CurChanDataPtr),y
    txa
    iny
    adc (CurChanDataPtr),y
    sta (CurChanDataPtr),y
++
  lda TempF4+1 : bmi +
  lda UnknownEF
  lsr : bcc +
  rts
+
  ldy #CHNVAR_VibratoBuffer+0
  lda (CurChanDataPtr),y
  and #$7f
  bne +
  jmp +++
+
  ldy #CHNVAR_VibratoVar1
  cmp (CurChanDataPtr),y
  beq +
  jmp +++
+
  lda #$00                    ;If VibratoVar1 was 0
  sta (CurChanDataPtr),y
  tax
  ldy #CHNVAR_VibratoBuffer+1
  lda (CurChanDataPtr),y
  rol
  rol
  rol
  rol
  and #$07
  sta TempF4

  ldy #CHNVAR_VibratoVar2
  lda (CurChanDataPtr),y
  bpl +
    lda #0  ;If VibratoVar2 had high bit set, negate TempF4 and do "dex"
    sec
    sbc TempF4
    sta TempF4
    dex
+ lda TempF4
  clc

  ; Adds A,X to CHNVAR_PeriodWord
  ldy #CHNVAR_PeriodWord
  adc (CurChanDataPtr),y : sta (CurChanDataPtr),y
  iny
  txa
  adc (CurChanDataPtr),y : sta (CurChanDataPtr),y

  ldy #CHNVAR_VibratoBuffer+1
  lda (CurChanDataPtr),y : and #$1F : sta TempF4
  
  clc
  ldy #CHNVAR_VibratoVar2
  lda (CurChanDataPtr),y : adc #$01 : sta (CurChanDataPtr),y  ;Adds 1 to VibratoVar2
  and #$7F
  cmp TempF4
  bne +++
  lda (CurChanDataPtr),y : and #$80 : sta (CurChanDataPtr),y
  ldy #CHNVAR_VibratoBuffer+0
  lda (CurChanDataPtr),y : eor #$80 : sta (CurChanDataPtr),y : bpl +++
  ldy #CHNVAR_VibratoVar2
  lda (CurChanDataPtr),y : eor #$80 : sta (CurChanDataPtr),y

+++
  ; $F5 points to VibratoCtrl or VibratoCtrlSFX|$80
  
  lda TempF4+1 : and #$7F : sta TempF4+1
  inc TempF4+1 ; Now points to NotePeriodWord or NotePeriodWordSFX (no high bit)

  ; Adds PeriodWord into NotePeriodWord or NotePeriodWordSFX
  
  clc
  
  ldy #CHNVAR_PeriodWord+0
  lda (CurChanDataPtr),y : ldy TempF4+1 : adc (CurChanDataPtr),y
  tax
  inc TempF4+1

  ldy #CHNVAR_PeriodWord+1
  lda (CurChanDataPtr),y : ldy TempF4+1 : adc (CurChanDataPtr),y
  sta TempF4

  ; X,Y now contains (NotePeriodWord + Period)

  lda #1
  cmp CurrentChannelNumber
  bne +

    ; This part is ignored for noise channel
    lda #15 ;enable all channels
    sta $4015
    txa
    and #15
    tax
    inc TempF4+1 ; Now points into DutyCycle or DutyCycleSFX
    ldy TempF4+1
    lda (CurChanDataPtr),y
    and #$80
    sta TempF4
    txa
    ora TempF4
    tax
    ldy #$00
    sty TempF4
  +
  ; Write period X,TempF4 (hi only written if changed)
  txa

  ldy CurrentChannelNumber
  ldx ChannelOffsets-1,y
  sta $4002,x     ;write wavelength low byte

  lda TempF4
  ldy #CHNVAR_PeriodHiOld
  cmp (CurChanDataPtr),y
  bne +
  rts
+ sta (CurChanDataPtr),y
  ora #$08
  sta $4003,x     ;write wavelength high byte
  rts


ResetPitch
  ; Input: TempF4 = CHNVAR_DutyCycleSFX
  ;              or CHNVAR_DutyCycle
  ;        will be written to TremoloVar2
  ;
  jsr LoadChanDataPtr_X
  lda AudioStatusBuffer+CHNVAR_VibratoBuffer+0,x
  and #$7F
  sta AudioStatusBuffer+CHNVAR_VibratoBuffer+0,x
  lda AudioStatusBuffer+CHNVAR_VibratoBuffer+2,x
  bpl ++
    ldy TempF4
    lda (CurChanDataPtr),y

