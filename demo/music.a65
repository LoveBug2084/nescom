   ; it by setting the delay in vibrato flags.
  pla
  ror
  ror
  ror
  ror
  and #$E0        ; Set high 3 bits from param
  sta TempF4
  ldy #CHNVAR_VibratoDefs
  lda (CurChanDataPtr),y
  and #$1F
  ora TempF4
  sta (CurChanDataPtr),y
  rts

